# LLM configuration (local, offline)
LLM_PROVIDER=ollama
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama3

# Speech-to-Text
# Using local Whisper (no API)
USE_WHISPER_API=false

# Text-to-Speech
# Free, local TTS
TTS_PROVIDER=edge

# Optional: ElevenLabs (only if enabled)
# ELEVENLABS_API_KEY=your_key_here

# Server
WS_HOST=0.0.0.